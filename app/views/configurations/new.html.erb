<h1>New configuration</h1>

<% form_for(@configuration) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :config_key %><br />
    <%= f.text_field :config_key %>
  </p>
  <p>
    <%= f.label :config_value %><br />
    <%= f.text_field :config_value %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', configurations_path %>